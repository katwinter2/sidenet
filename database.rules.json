{
  "rules": {
    "feed": {
      ".read": true,
      ".write": true,
      ".indexOn": ["timestamp", "url"]
    },
    "explorations": {
      ".read": true,
      ".write": true
    },
    "users": {
      "$uid": {
        ".read": "auth !== null && auth.uid === $uid",
        ".write": "auth !== null && auth.uid === $uid"
      }
    }
  }
}
